  	<servlet-class>com.mvc.controller.UpdateServletAdd</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>RegisterServlet</servlet-name>
  	<url-pattern>/RegisterServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>TechServlet</servlet-name>
  	<url-pattern>/TechServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>updateServlet</servlet-name>
  	<url-pattern>/updateServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateServletAdd</servlet-name>
  	<url-pattern>/UpdateServletAdd</url-pattern>
  </servlet-mapping>
</web-app>
